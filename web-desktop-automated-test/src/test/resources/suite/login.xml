<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Login regression tests" configfailurepolicy="continue">

	<listeners>
		<listener class-name="util.TestListener" />
	</listeners>

	<test name="Login - windows 10 - Chrome 84">
		<parameter name="os" 				value="Windows" />
		<parameter name="os_version" 		value="10" />
		<parameter name="resolution" 		value="1280x800" />
		<parameter name="browser" 			value="Chrome" />
		<parameter name="browser_version" 	value="84" />
		<parameter name="project" 			value="login - win10 chrome84" />
		<parameter name="build"   			value="win10 chrome84 resolution 1280x800" />
		<classes>
			<class name="test.LoginTest" />
		</classes>
	</test>
</suite>